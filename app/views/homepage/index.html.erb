<nav>

  <h1 id='logo'>Fhellow</h1>
  <ul>
  <li><a href="#">About</a></li>
  <li><a href="#">Activity</a></li>
  <li><a href="#">Post</a></li>
  <li><%= link_to "Join", new_user_registration_path %></li>
  <li><%= link_to "Log in", new_user_session_path %></li>
  </ul>
</nav>

<div id='map'></div>
<% if @users.any? %>
	<% @users.each do |user| %>
		<% if user.longitude && user.latitude %>
			<div class='lat'><%= user.latitude %></div>
			<div class='lng'><%= user.longitude %></div>
		<% end %>
	  <%= image_tag user.avatar.url(:thumb), class: "avatar-image" %>
	  <h3><%= user.name_first %> <%= user.name_last %></h3>
	  <h4><%= user.job_title %></h4>
	  <p><%= user.location %></p>
	  <p><%= user.bio %></p>
	<% end %>
<% else %>
	<p>This is where users will show</p>
<% end %>


